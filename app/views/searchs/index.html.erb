<div class= "container d-flex justify-content-evenly ">
    <div class ='flex-1'style="background-color:red;width:300px">
       <label for="customRange1" class="form-label">Example range</label>
        <input type="range" class="form-range" id="customRange1">   
        
    </div>
    <%=render "index"%>
   
    <div class="flex-1" style="background-color:red;width:300px">
 
            <%= form_tag search_path, method: 'get' do %>
              <%= select_tag( :brand, options_from_collection_for_select(Brand.all, "id", "name"))%>
              <%= submit_tag 'Filter'%>
            <% end %>

    </div>
</div>
<nav aria-label="Page navigation example">
  <ul class="pagination">
    <li class="page-item">
      <%=link_to "search?search=#{params[:search]}&page=#{@page-1}",class:"page-link" ,"aria-label"=>"Previous" do %>
            <span aria-hidden="true">&laquo;</span>
        <%end%>
    </li>
    <%(0).upto((@searchcount)/2) do |i|%>
    <li class="page-item"><%=link_to i,"search?search=#{params[:search]}&page=#{i}",class:"page-link" %></li>    
    <%end%>
    <li class="page-item"> 
        <%=link_to "search?search=#{params[:search]}&page=#{@page+1}",class:"page-link" ,"aria-label"=>"Next" do %>
            <span aria-hidden="true">&raquo;</span>
        <%end%>
    </li>
  </ul>
</nav>