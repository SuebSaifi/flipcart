<%= form_for(@order) do |f| %>
  <p>
    <%= f.hidden_field :user_id ,value: current_user.id%>
    <%= f.label :name %><br>
    <%= f.text_field :name, required: true, autofocus: true ,value: current_user.first_name%>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email,value: current_user.email %>
  </p>
  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end%>