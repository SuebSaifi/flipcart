<%# <%debugger%> %>
<%= form_for @order, :url => user_cart_order_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name,required: true, autofocus: true ,value: current_user.first_name.capitalize%>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email,value: current_user.email %>
  </p>
  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address , required: true%>
  </p>
   <p>
    <%= f.hidden_field :cart_id,value: @cart%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end%>