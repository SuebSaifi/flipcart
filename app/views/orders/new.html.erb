<div class="container mx-auto my-4"style="width:400px">
<div class="container mx-auto my-4"style="width:500px">
<%=form_for(@order,html:{"data-turbo"=>'false'}) do |f| %>
  <%=render :partial => "#{@order.current_step}" ,:locals=>{:f=>f}%>
  <div class="actions my-4">
    <%= f.submit "Continue" %>
  </div>
  <div class="actions my-4">
    <%= f.submit "back",name:"back_btn" unless @order.first_step? %>
  </div>
<% end %>

</div>
</div>
<>