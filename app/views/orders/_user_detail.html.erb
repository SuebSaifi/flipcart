<h1>User Shipping Details</h1>
<div class="card">
    <div class="card-body"> 
        <div class="my-3">
            <%=f.label :name%>
            <%=f.text_field :name,value:current_user.first_name%>
        <div>
        <div class="my-3">
            <%=f.label :email%>
            <%=f.text_field :email,value: current_user.email%>
        </div>
        <div class="my-3">
            <%=f.label :phone_number%>
            <%=f.text_field :phone_number,vlaue:current_user.phone_number%>
        </div>
        <div class="my-3">
            <%=f.label :address%>
            <%=f.text_field :address%>
        </div>
        <div class="my-3">
            <%=f.label :pincode%>
            <%=f.text_field :pincode%>
        </div>
        <div class="my-3">
            <%=f.label :state%>
            <%=f.text_field :state%>
        </div>
        <%=f.hidden_field :user_id,value: current_user.id%>
        </div>
    </div>
</div>