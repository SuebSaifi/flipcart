<%Category.all.each do |cat|%>
<tr>
    <td>
        <%=cat.name%>
    </td>
     <td>
        <%=link_to "Edit" , edit_admin_category_path(cat),class:"btn btn-info"%>
    </td>
     <td>
         <%=button_to "Delete" , admin_category_path(cat),method: :delete,class:"btn btn-danger"%>
    </td>
</tr>
<%end%>