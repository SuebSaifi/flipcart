<%Product.all.each do |product|%>
<tr>
    <td>
        <%=product.title%>
    </td>
    <td>
        <%=product.description%>
    </td>
    <td>
        <%=product.price%>
    </td>
     <td>
        <%=link_to "Edit" , edit_admin_product_path(product),class:"btn btn-info"%>
    </td>
     <td>
         <%=button_to "Delete" , "/admin/products/#{product.id}",method: :delete,class:"btn btn-danger" %>
    </td>
</tr>
<%end%>