<%Product.all.each do |product|%>
<tr>
    <td>
        <%=product.title%>
    </td>
    <td>
        <%=product.description%>
    </td>
    <td>
        <%=product.price%>
    </td>
     <td>
        <%=link_to "Edit" , edit_admin_product_path(product)%>
    </td>
     <td>
         <%=button_to "Delete" , "/admin/products/#{product.id}",method: :delete %>
    </td>
</tr>
<%end%>